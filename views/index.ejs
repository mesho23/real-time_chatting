    <div class="container">
      <div class="login">
       
        <div id="loginArea">  <button id="login" >login</button>
          <button id="register" >register</button> </div>
          
      
      </div>
      
      <div class="chatcon">
      
      
        <div id="chatBox"></div>

        <div class="text_controls">

        
          <span id="message" contenteditable></span>
          
          
          <button
            class="messageBtn" id="sendBtn"
          >
            <image src ="./icons/send.svg " ></image>
          </button>
          <button   class="messageBtn" id="filebtn">
          <input id="file" type="file" onchange="sendfile(this.files)" />
          
          <label for="file"> <image id="send_fileIMG" src = "./icons/send_file.svg" ></image>  </label>
        </button>
        
        
      </div>
     
      </div>
    
      <div class="join">
     
        <input id="channel" name="channel" type="text" placeholder="Join a room" /><button id="joinBtn">
          join
        </button>
        <button id="AboutUs" >About us</button>
    </div>
     
   
     
      <div id="login-card-con">
      <div class="login-card" id="login-card" >
        <div class="card-header">
          <div class="log">Login</div>
        </div>
       
          <div class="form-group">
            <label for="username">Username:</label>
            <input required="" name="username" id="username" type="text" required>
          </div>
          <div class="form-group" id="emailcon" style="display: none;">
            <label for="email">Email:</label>
            <input required="" name="email" id="email" type="email" required> 
          </div>
          <div class="form-group">
            <label for="password">Password:</label>
            <input required="" name="password" id="pass" type="password"  pattern="^(?=.*[A-Za-z])(?=.*\d)(?=.*[@$!%*#?&])[A-Za-z\d@$!%*#?&]{8,}$" required>
          </div> <div class="form-group" >                                         
            <label id="forgetPassword" style="display: none;">forget Password?</label>
            
          </div>
          <p class="error"></p>
          <div class="form-group">
            <button id="userBtn" class="userbtn">Login</button>
            <button class="loading userbtn" style="display: none;"><div class="spinner" class="spinner center">
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
              <div class="spinner-blade"></div>
          </div></button>          
            
            
      
          </div>
      
      </div></div>
      
      
      <div id="forgetpass-card-con" style="display: none;">
        <div class="login-card" >
          <div class="card-header">
            <div class="log">Password reset</div>
          </div>

          <div class="form-group" id="forget-pass-group" >
           
            <input required="" placeholder="Email" name="email-forget-pass" id="email-forget-pass" type="email" required> 
          </div>
            
          <div class="form-group" id="OTP-group" style="display: none;">
            
            <input required="" name="otp" id="otp" type="text" placeholder="OTP"  required>
            
          </div> 

          <div class="form-group" id="new_pass" style="display: none;">
           
            <input required="" name="password" id="newpassinput" placeholder="New Password" type="password"  pattern="^(?=.*[A-Za-z])(?=.*\d)(?=.*[@$!%*#?&])[A-Za-z\d@$!%*#?&]{8,}$" required>
           
            <input required="" name="password" id="passcheck" type="password" placeholder="Re-type New Password" pattern="^(?=.*[A-Za-z])(?=.*\d)(?=.*[@$!%*#?&])[A-Za-z\d@$!%*#?&]{8,}$" required>
          </div> 
          
          <p class="error"></p>
            <div class="form-group">
              <button id="newpassBtn" class="userbtn" style="display: none;">Submit New Password</button>
              <button id="OTPBtn" class="userbtn" style="display: none;">Submit OTP</button>
              <button id="Pass-resetBtn" class="userbtn">Reset Password</button>
              <button class="loading" class="userbtn" style="display: none;"><div class="spinner" class="spinner center">
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
                <div class="spinner-blade"></div>
            </div></button>
              
              
        
            </div>
        
        </div>
        
    </div>
   

